name: basic_cleaning
conda_env: conda.yml

entry_points:
  main:
    parameters:

      input_artifact:
        description: None
        type: string

      output_name:
        description: None
        type: string

      output_type:
        description: None
        type: string

      output_description:
        description: None
        type: string

      min_price:
        description: None
        type: int

      max_price:
        description: None
        type: int

    command: "python run.py  --input_artifact {input_artifact} --output_name {output_name}  --output_type {output_type}  --output_description {output_description} --min_price {min_price} --max_price {max_price}"
